<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>customer</display-name>
  <servlet>
    <servlet-name>AddCustomerServlet</servlet-name>
    <servlet-class>cn.web.controller.AddCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListCustomerServlet</servlet-name>
    <servlet-class>cn.web.controller.ListCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EditCustomerServlet</servlet-name>
    <servlet-class>cn.web.controller.EditCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DelCustomerServlet</servlet-name>
    <servlet-class>cn.web.controller.DelCustomerServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>AddCustomerServlet</servlet-name>
    <url-pattern>/servlet/AddCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListCustomerServlet</servlet-name>
    <url-pattern>/servlet/ListCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditCustomerServlet</servlet-name>
    <url-pattern>/servlet/EditCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DelCustomerServlet</servlet-name>
    <url-pattern>/servlet/DelCustomerServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>